<Application x:Class="StorageSystem.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:fui="clr-namespace:FloxelLib;assembly=Floxel"
             xmlns:local="clr-namespace:StorageSystem"
             xmlns:common="clr-namespace:StorageSystem.Common"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <common:InverseBooleanConverter x:Key="InvertBool"/>
            <ResourceDictionary.MergedDictionaries>
                <fui:Resources/>
                <ResourceDictionary Source="Assets/StorageIcon.xaml"/>
                <ResourceDictionary Source="Assets/LogoutIcon.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
